{
  "env": {
    "node": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn"],
    "require-await": ["warn"],
    "no-unused-vars": 0,
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ]
  }
}
